package com.j2.w10;

public class LightUndo{
  int level;
  public void dim(int level){
    this.level=level;
    if (level == 0){
      off();
    } else {
      System.out.println("Light is dimmed to " +level);
    }
  }
  public int getLevel(){
    return level;
  }
  public Light() {}
  public void on(){
    System.out.println("light is on");
  }
  public void off(){
    System.out.println("light is off");
  }
}
